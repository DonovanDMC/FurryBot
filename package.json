{
  "name": "furrybot",
  "version": "11.0.0",
  "buildDate": null,
  "description": "A fun bot for furry Discord servers!",
  "main": "index.js",
  "scripts": {
    "circ": "dpdm --tree=false --circular=true --warning=false -T .",
    "update": "git pull && git submodule update --recursive --remote",
    "lint": "eslint src --ext .ts --fix",
    "build": "npm run lint && rm -rf build && tsc && echo Build Finished",
    "start": "node .",
    "start:prod": "MODE=prod node .",
    "start:beta": "MODE=beta node .",
    "dev": "ts-node src"
  },
  "repository": {
