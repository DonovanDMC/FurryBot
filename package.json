    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "@uwu-codes/eslint-config": "^1.0.17",
    "@uwu-codes/tsconfig": "^1.0.1",
    "discord-api-types": "^0.18.1",
    "dpdm": "^3.6.1",
    "eslint": "^7.27.0",
    "typescript": "^4.3.1-rc"
