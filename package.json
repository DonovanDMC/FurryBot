  "bugs": {
    "url": "https://github.com/FurryBotCo/FurryBot/issues"
  },
  "homepage": "https://github.com/FurryBotCo/FurryBot#readme",
  "engines": {
    "node": ">=16.x",
    "npm": ">=7.x"
  },
  "os": [
    "linux"
  ],
  "engineStrict": true,
  "dependencies": {
    "@drep/api": "^2.0.3",
    "chunk": "^0.0.3",
    "clustering": "npm:@uwu-codes/clustering@^1.0.11",
