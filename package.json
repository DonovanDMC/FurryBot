    "mongodb": "^3.6.8",
    "ms": "^2.1.3",
    "node-fetch": "^2.6.1",
    "slash-commands": "^1.4.0",
    "slash-extras": "npm:@uwu-codes/discord-slash-commands@^1.0.4",
    "source-map-support": "^0.5.19",
    "trello-helper": "^3.0.7",
